{
  "name": "zeal-io/paymob",
  "description": "Process Payment With Paymob",
  "keywords": [
    "Paymob",
    "Paymob Payment",
    "Paymob Provider",
    "Paymob Payment Provider",
    "Payemnt",
    "Paymob Gateway",
    "package"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bellal Mohamed",
      "email": "b.mohamed@myzeal.app"
    }
  ],
  "type": "package",
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^8.12|^9.0|^10.0",
    "guzzlehttp/guzzle": "^7.8",
    "zeal-io/zeal-payment-framework": "dev-master"
  },
  "require-dev": {
  "nunomaduro/phpinsights": "^2.0",
  "squizlabs/php_codesniffer": "^3.6",
  "orchestra/testbench": "^7.0",
  "symfony/var-dumper": "^6.0",
  "filp/whoops": "^2.13",
  "pestphp/pest": "^1.22.3",
  "pestphp/pest-plugin-faker": "^1.0",
  "pestphp/pest-plugin-laravel": "^1.0"
},
  "autoload": {
    "psr-4": {
      "Zeal\\Paymob\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zeal\\LaravelStripe\\Tests\\": "tests/",
      "Workbench\\App\\": "workbench/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zeal\\Paymob\\PaymobServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../Zeal-Payment-Framework"
    }
  ]
}
